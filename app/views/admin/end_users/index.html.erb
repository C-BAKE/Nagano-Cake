<div class = "container">
  <div class = "row">
    <h2>会員一覧</h2>
    <table class = "table">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>販売ステータス</th>
        </tr>
      </thead>

      <tbody>
        <% @end_users.each do |end_user| %>
        <tr>
          <td><%= end_user.id %></td>
          <td><%= link_to admin_end_user_path(end_user) do %><%= end_user.last_name %><%= end_user.first_name %><% end %></td>
          <td><%= end_user.email %></td>
          <td>
					<% if end_user.is_deleted? %>
						<span class="text-active">有効</span>
					<% else %>
						<span class="text-invalid">退会済み</span>
					<% end %>
				  </td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <%= paginate @end_users %>
  </div>
</div>